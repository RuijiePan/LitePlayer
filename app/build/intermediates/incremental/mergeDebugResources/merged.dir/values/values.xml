<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="main">#FF31C27C</color>
    <color name="main_dark">#FF535353</color>
    <declare-styleable name="Indicator"><attr format="color|reference" name="color"/><attr format="dimension|reference" name="height"/></declare-styleable>
    <declare-styleable name="Lrc"><attr format="dimension" name="textSize"/><attr format="reference|color" name="normalTextColor"/><attr format="reference|color" name="currentTextColor"/><attr format="dimension" name="dividerHeight"/><attr format="integer" name="rows"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">liteplayer</string>
    <string name="hello_world">Hello world!</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.

        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="popwin_anim">
        <item name="android:windowEnterAnimation">@anim/pop_show_anim</item>
        <item name="android:windowExitAnimation">@anim/pop_gone_anim</item>
    </style>
